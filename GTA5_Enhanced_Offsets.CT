local symbols = {"WorldPTR", "GlobalPTR", "ReplayInterfacePTR", "WayPointPTR", "BlipPtr", "LocalScriptsPTR", "CNetworkPlayerMgrPTR", "AimCPedPTR", "GTAPlusPTR"}
for _, symbol in ipairs(symbols) do
  unregisterSymbol(symbol)
end
